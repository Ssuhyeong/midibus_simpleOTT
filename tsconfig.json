{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@apis/*": ["apis/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@constants/*": ["constants/*"],
      "@navigations/*": ["navigations/*"],
      "@screens/*": ["screens/*"],
      "@styles/*": ["styles/*"],
      "@types/*": ["types/*"],
      "@env": ["./@types/env.d.ts"]
    },
    "target": "es5",
    "lib": ["dom", "es2015"],
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  },
  "include": ["src", "env.d.ts"]
}
